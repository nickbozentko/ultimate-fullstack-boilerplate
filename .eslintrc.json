{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "indent": [
            "warning",
            4
        ],
        "semi": [
            "warning",
            "always"
        ],
        "react/jsx-filename-extension": "off"
    }
}